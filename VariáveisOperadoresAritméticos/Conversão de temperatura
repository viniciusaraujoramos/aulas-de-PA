// Data atual  : 09/03/2023
Var
F, C: real

Inicio
// entrada

 escreval (" escreva a temperatura em celsius: ")
 leia (C)

//processamento
F <- ((9*C) + 160)/5



//final
 escreval (" a temperatura em fahrenheit é: " , F)





Fimalgoritmo
